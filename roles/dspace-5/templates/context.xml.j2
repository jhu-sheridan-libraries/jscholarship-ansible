<?xml version='1.0'?>
<Context
  docBase="{{ dspace_install }}/webapps/{{ item.name }}"
{% if item.default_webapp | default(false) %}
  useRelativeRedirects="false"
{% endif %}
  reloadable="{{ dspace_context_reloadable }}" >
  <Resources cachingAllowed="{{ dspace_context_caching_allowed }}" />
</Context>
